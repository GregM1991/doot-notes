{
  "name": "note-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node build",
    "dev": "cross-env MOCKS=true vite dev",
    "build": "vite build",
    "build:icons": "bun ./other/build-icons.ts",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "setup": "npm run build && prisma generate && prisma migrate deploy && prisma db seed && playwright install",
    "format": "prettier --write .",
    "prisma:studio": "prisma studio",
    "seed": "bun run ./prisma/seed.ts",
    "test": "npx --node-options=\"--experimental-vm-modules\" jest src",
    "test:watch": "npx --node-options=\"--experimental-vm-modules\" jest src --watch",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@csstools/postcss-global-data": "^3.0.0",
    "@iconify/json": "^2.2.190",
    "@melt-ui/pp": "^0.3.2",
    "@melt-ui/svelte": "^0.81.0",
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-node": "^5.2.2",
    "@sveltejs/kit": "^2.5.25",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/svelte": "^5.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.10",
    "@types/cookie": "^0.6.0",
    "@types/crypto-js": "^4.2.2",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/qrcode": "^1.5.5",
    "@vitest/ui": "^2.1.5",
    "autoprefixer": "^10.4.20",
    "chalk": "^5.3.0",
    "enforce-unique": "^1.3.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "fs-extra": "^11.2.0",
    "globals": "^15.0.0",
    "jsdom": "^24.1.0",
    "msw": "^2.2.13",
    "postcss": "^8.4.45",
    "postcss-csso": "^6.0.1",
    "postcss-preset-env": "^10.0.2",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.6",
    "prisma": "^5.20.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-preprocess": "^6.0.2",
    "sveltekit-rate-limiter": "^0.5.1",
    "sveltekit-superforms": "^2.18.0",
    "tslib": "^2.4.1",
    "tsx": "^4.19.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.6.0",
    "unplugin-icons": "^0.18.5",
    "vite": "^5.0.3",
    "vitest": "^2.1.5"
  },
  "type": "module",
  "dependencies": {
    "@epic-web/totp": "^1.1.2",
    "@faker-js/faker": "^8.4.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.20.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.3.0",
    "classnames": "^2.5.1",
    "close-with-grace": "^1.3.0",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.5.0",
    "dotenv": "^16.4.5",
    "execa": "^8.0.1",
    "jsonwebtoken": "^9.0.2",
    "qrcode": "^1.5.3",
    "remix-auth": "^3.6.0",
    "zod": "^3.23.8"
  },
  "msw": {
    "workerDirectory": [
      "static"
    ]
  },
  "dockerfile": {
    "packages": {
      "deploy": [
        "sqlite3"
      ]
    }
  }
}
