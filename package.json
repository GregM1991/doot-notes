    "unplugin-icons": "^0.18.5",
    "vite": "^5.0.3",
    "vitest": "^1.2.0"
  },
  "type": "module",
  "dependencies": {
    "@epic-web/remember": "^1.0.2",
    "@faker-js/faker": "^8.4.1",
    "@prisma/client": "latest",
    "@types/fs-extra": "^11.0.4",
