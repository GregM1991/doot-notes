    "prisma:studio": "prisma studio",
    "seed": "bun run ./prisma/seed.ts"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.190",
    "@playwright/test": "^1.28.1",
    "@remix-run/eslint-config": "2.7.2",
